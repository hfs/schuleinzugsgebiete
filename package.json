{
  "name": "schuleinzugsgebiete",
  "version": "0.1.0",
  "description": "School districts in Potsdam",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-data-potsdam/schuleinzugsgebiete.git"
  },
  "author": "Jan Ehrhardt <jan.ehrhardt@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/open-data-potsdam/schuleinzugsgebiete/issues"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "jquery": "^3.2.1",
    "leaflet": "^1.0.3",
    "node-sass": "^4.5.3",
    "tachyons": "^4.7.4"
  },
  "scripts": {
    "postinstall": "scripts/copy-deps && node-sass scss/styles.scss resources/schuleinzugsgebiete/public/css/styles.css --output-style compressed && babel js/app.js --out-file resources/schuleinzugsgebiete/public/js/app.js",
    "watch-sass": "node-sass --source-map true -w scss/styles.scss resources/schuleinzugsgebiete/public/css/styles.css",
    "watch-js": "babel -w js/app.js --out-file resources/schuleinzugsgebiete/public/js/app.js --source-maps"
  }
}
